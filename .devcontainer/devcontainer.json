{
  "name": "Flutter Dev Container",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "flutter-dev",
  "workspaceFolder": "/app",
  "shutdownAction": "stopCompose",
  "remoteUser": "root",
  "postAttachCommand": "flutter doctor",
  "customizations": {
    "vscode": {
      "extensions": [
        "dart-code.dart-code",
        "dart-code.flutter",
        "ms-vscode-remote.remote-containers"
      ]
    }
  }
}
